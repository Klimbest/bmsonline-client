<div
        {% if (gadgetProgressBar.eventLink is not empty) %}
            onclick="ajaxChangePage({{ gadgetProgressBar.eventLink.page.id }});"
        {% endif %}
        {% if gadgetProgressBar.tooltip %}
            title="{{ gadgetProgressBar.name }}"
        {% endif %}
        name="{{ gadgetProgressBar.name }}"
        id="{{ gadgetProgressBar.id }}" class="bms-panel bms-gadgetprogressbar"
        style="
                top: {{ gadgetProgressBar.topPosition }}px;
                left: {{ gadgetProgressBar.leftPosition }}px;
                width: {{ gadgetProgressBar.width }}px;
                height: {{ gadgetProgressBar.height }}px;
                line-height: {{ gadgetProgressBar.height }}px;
                z-index: {{ gadgetProgressBar.zIndex }};
        {% if (gadgetProgressBar.eventLink is not empty) %}
                cursor: pointer;
        {% endif %}
                ">

    {% set optimumMin = (gadgetProgressBar.optimumMin-gadgetProgressBar.rangeMin)/(gadgetProgressBar.rangeMax-gadgetProgressBar.rangeMin)*100|round(2, 'common') %}
    {% set optimumMax = (gadgetProgressBar.optimumMax-gadgetProgressBar.rangeMin)/(gadgetProgressBar.rangeMax-gadgetProgressBar.rangeMin)*100|round(2, 'common') %}
    {% if gadgetProgressBar.setRegisterId is not null %}
        {% set setPoint = (gadgetProgressBar.setRegisterId.registerCurrentData.fixedValue-gadgetProgressBar.rangeMin)/(gadgetProgressBar.rangeMax-gadgetProgressBar.rangeMin)*100|round(2, 'common') %}

    {% endif %}
    {% set valuePoint = (gadgetProgressBar.valueRegisterId.registerCurrentData.fixedValue-gadgetProgressBar.rangeMin)/(gadgetProgressBar.rangeMax-gadgetProgressBar.rangeMin)*100|round(2, 'common') %}
    <div class="progress" style="margin-bottom: 0; overflow: hidden">
        <div id="value{{ gadgetProgressBar.valueRegisterId.id }}"
             style="position: absolute; top:-1px; left: {{ valuePoint }}%; height: 50%; z-index: 15; border-left: 2px solid black;  color: black; margin-bottom: 10px;">

        </div>
        {% if gadgetProgressBar.setRegisterId is not null %}
            <div id="set{{ gadgetProgressBar.setRegisterId.id }}"
                 style="position: absolute; top: -3px; left: {{ setPoint }}%; height: 60%; z-index: 14; border-left: 4px solid green; color: green">
            </div>
        {% endif %}
        <div class="progress-bar" role="progressbar"
             style="width: {{ optimumMin }}%; background-color: {{ gadgetProgressBar.color1 }}">
        </div>
        <div class="progress-bar" role="progressbar"
             style="width: {{ optimumMax - optimumMin }}%; background-color: {{ gadgetProgressBar.color2 }}">
        </div>
        <div class="progress-bar" role="progressbar"
             style="width: {{ 100-optimumMax }}%; background-color: {{ gadgetProgressBar.color3 }}">
        </div>
    </div>
    <div class="progress progress-label">
        <div id="rangeMin" class="text-right" style="width:3%; float:left;">
            {{ gadgetProgressBar.rangeMin|round(2, 'common') }}
        </div>
        <div class="text-right" style="width:{{ optimumMin }}%; float:left">
            {{ gadgetProgressBar.optimumMin|round(2, 'common') }}
        </div>
        <div class="text-right" style="width: {{ optimumMax - optimumMin }}%; float:left">
            {{ gadgetProgressBar.optimumMax|round(2, 'common') }}
        </div>
        <div id="rangeMax" class="text-right" style="width: {{ 100-optimumMax-3 }}%; float:left">
            {{ gadgetProgressBar.rangeMax|round(2, 'common') }}
        </div>
    </div>

    {% if labels %}
        <span class="label label-primary label-bms-panel">
            <i class="fa fa-font fa-fw"></i> {{ gadgetProgressBar.id }}
            <a href="{{ path('gadgetprogressbar_copy', {'id': gadgetProgressBar.id }) }}"><i
                        class='fa fa-fw fa-clone fa-blue'></i></a>
            <a href="#"><i class='fa fa-fw fa-bolt fa-yellow'></i></a>
            <a href="{{ path('gadgetprogressbar_edit', {'id': gadgetProgressBar.id }) }}"><i
                        class='fa fa-fw fa-cogs fa-yellow'></i></a>
            <a href="{{ path('gadgetprogressbar_delete', {'id': gadgetProgressBar.id }) }}"><i
                        class='fa fa-fw fa-trash-o fa-red'></i></a>
        </span>
    {% endif %}
</div>