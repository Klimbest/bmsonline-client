<div
        {% if (inputButton.eventLink is not empty) %}
            onclick="ajaxChangePage({{ inputButton.eventLink.page.id }});"
        {% endif %}
        name="{{ inputButton.name }}"
        id="{{ inputButton.id }}" class="bms-panel bms-inputbutton"
        style="
                top: {{ inputButton.topPosition }}px;
                left: {{ inputButton.leftPosition }}px;
                width: {{ inputButton.width }}px;
                height: {{ inputButton.height }}px;
                line-height: {{ inputButton.height }}px;
                z-index: {{ inputButton.zIndex }};
        {% if (inputButton.eventLink is not empty) %}
                cursor: pointer;
        {% endif %}
                ">
    INPUT BUTTON
    {% if labels %}
        <span class="label label-primary label-bms-panel">
            <i class="fa fa-font fa-fw"></i> {{ inputButton.id }}
            <a href="{{ path('inputbutton_copy', {'id': inputButton.id }) }}"><i class='fa fa-fw fa-clone fa-blue'></i></a>
            <a href="{{ path('inputbutton_edit', {'id': inputButton.id }) }}"><i class='fa fa-fw fa-cogs fa-yellow'></i></a>
            <a href="{{ path('inputbutton_delete', {'id': inputButton.id }) }}"><i class='fa fa-fw fa-trash-o fa-red'></i></a>
        </span>
    {% endif %}
</div>