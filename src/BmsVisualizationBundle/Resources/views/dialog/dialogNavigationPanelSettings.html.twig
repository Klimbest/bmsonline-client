<div class="dialog-navigation-panel-settings" title="Właściwości panelu nawigacyjnego:">
    <div class="form-group">
        <div class="row">            
            <div class="col-md-5">
                <span class="label label-primary">Adres nowego odnośnika:</span>
                <div class="row">
                    <div class="col-md-8">
                        <label for="pages">Strony:</label>
                        <select class="form-control pages">
                            {% for page in pages %}
                                <option value="{{ page.id }}">{{ page.name }}</option>
                            {% endfor %}
                        </select>
                    </div>
                </div>
            </div>
