                        <button type="button" class="btn btn-default btn-xs disabled"><i class="fa fa-question fa-fw"></i></button>
                        <button type="button" class="btn btn-default btn-xs disabled"><i class="fa fa-question fa-fw"></i></button>
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-5">
                        <label for="fontFamily">Styl:</label>
                        <select id="fontFamily" class="form-control font-family">
                            {% set fonts = ["Times New Roman",
                                "Arial",
                                "Tahoma",
                                "Verdana"]%}
                            {% for font in fonts %}
                                <option value="{{font}}">{{font}}</option>
                            {% endfor %}
                        </select>
                    </div>
                    <div class="col-md-3">
                        <label for="fontSize">Rozmiar:</label>
                        <select id="fontSize" class="form-control font-size">
                            {% for i in 6..96 %}
                                <option value="{{i}}">{{i}}</option>
                            {% endfor %}
                        </select>
                    </div>
                    <div class="col-md-3">
                        <label for="fontColor">Kolor:</label>
                        <input id="fontColor" type="color" class="form-control"/>
                    </div>
                </div>
            </div>#}
            <div class="col-md-6 col-md-offset-1">
                <div class="row">
                    <div class="col-md-6">
                        <span class="label label-primary">Pozycja:</span>
                    </div>
                    <div class="col-md-6">
                        <span class="label label-primary">Rozmiar:</span>
                    </div>
                </div>
                <div class="row ">
                    <div class="col-md-3">
                        <label for="topPosition">Od góry:</label>
                        <input id="topPosition" type="number" class="form-control" value="50"/>
                    </div>
                    <div class="col-md-3">
                        <label for="leftPosition">Od lewej:</label>
                        <input id="leftPosition" type="number" class="form-control" value="50"/>
                    </div>
                    <div class="col-md-3">
                        <label for="width">Szerokość:</label>
                        <input id="width" type="number" class="form-control" value="100"/>
                    </div>
                    <div class="col-md-3">
                        <label for="height">Wysokość:</label>
                        <input id="height" type="number" class="form-control" value="100"/>
                    </div>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-md-6">
                <span class="label label-primary">Właściwości ramki:</span>
                <div class="row">
                    <div class="col-md-3">
                        <label for="borderWidth">Grubość:</label>
                        <input id="borderWidth" type="number" class="form-control" value="2"/>
                    </div>
                    <div class="col-md-5">
                        <label for="borderStyle">Styl:</label>
                        <select id="borderStyle" class="form-control">
                            <option value="solid">Ciągła</option>
                            <option value="dotted">Wykropkowana</option>
                            <option value="dashed">Wykreskowana</option>
                        </select> 
                    </div>
                    <div class="col-md-3">
                        <label for="borderColor">Kolor:</label>
                        <input id="borderColor" type="color" class="form-control" />
                    </div>
                </div>
            </div>
            <div class="col-md-6">
                <div class="row">
                    <div class="col-md-4">
                        <span class="label label-primary">Właściwości tła:</span>
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-4">
                        <label for="backgroundColor">Kolor tła:</label>
                        <input id="backgroundColor" type="color" class="form-control"/>
                    </div>
                    <div class="col-md-8 form-group">
                        <label for="opacity">Przezroczystość tła:</label>
                        <input id="opacity" type="range" class="form-control" value="1" min="0" max="1" step="0.1"/>
                    </div>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-md-12 text-center">
                <span class="label label-primary">Ustawienie zaokrąglenia narożników:</span>
                <div class="row">
                    <div class="col-md-2 col-md-offset-1">
                        <form>
                            <input class="form-control" id="borderRadiusTL" type="number" name="txt_BR_TL" min="0" max="200" step="1" oninput="this.form.range_BR_TL.value=this.value"></label>
                            <input class="form-control" id="borderRadiusTL" type="range" name="range_BR_TL" min="0" max="200" step="1" oninput="this.form.txt_BR_TL.value=this.value"/> 
                        </form>
                    </div>
                    <div class="col-md-2 col-md-offset-6 text-right">
                        <form>
                            <input class="form-control" id="borderRadiusTR" type="number" name="txt_BR_TR" min="0" max="200" step="1" oninput="this.form.range_BR_TR.value=this.value"></label>
                            <input class="form-control" id="borderRadiusTR" type="range" name="range_BR_TR" min="0" max="200" step="1" oninput="this.form.txt_BR_TR.value=this.value"/> 
                        </form>
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-12 text-center">
                        <div class="dialog-panel"></div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-2 col-md-offset-1">
                        <form>
                            <input class="form-control" id="borderRadiusBL" type="number" name="txt_BR_BL" min="0" max="200" step="1" oninput="this.form.range_BR_BL.value=this.value"></label>
                            <input class="form-control" id="borderRadiusBL" type="range" name="range_BR_BL" min="0" max="200" step="1" oninput="this.form.txt_BR_BL.value=this.value"/> 
                        </form>
                    </div>
                    <div class="col-md-2 col-md-offset-6 text-right">
                        <form>
                            <input class="form-control" id="borderRadiusBR" type="number" name="txt_BR_BR" min="0" max="200" step="1" oninput="this.form.range_BR_BR.value=this.value"></label>
                            <input class="form-control" id="borderRadiusBR" type="range" name="range_BR_BR" min="0" max="200" step="1" oninput="this.form.txt_BR_BR.value=this.value"/> 
                        </form>
                    </div>
                </div>
            </div>
        </div>
        
    </div>
</div>